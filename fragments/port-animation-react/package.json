{
    "name": "port-animation-react",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "test": "jest",
        "start": "webpack-cli serve --config webpack.dev.config.js",
        "start:prod": "webpack-cli -p serve",
        "build": "webpack --mode production  --config webpack.prod.config.js && npm run copy-dist",
        "copy-dist": "copyfiles -u 1 dist/* ../../dist/port-modelling-fe/browser/assets/microservices/port-animation-react",
        "prettier": "prettier --write \"src/**/*.{ts,tsx,css}\"",
        "lint": "npm run eslint",
        "lint:prod": "npm run eslint:prod",
        "eslint": "eslint \"src/**/{*.tsx,*.ts}\" --fix",
        "eslint:prod": "eslint \"src/**/{*.tsx,*.ts}\"",
        "server": "tsc-watch ./server/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\" --onFailure \"echo Beep! Compilation Failed\" --compiler typescript/bin/tsc",
        "startWithMock": "concurrently \"npm:server\" \"npm:start\""
    },
    "devDependencies": {
        "@babel/preset-env": "7.14.4",
        "@babel/preset-react": "7.13.13",
        "@babel/preset-typescript": "^7.16.7",
        "@testing-library/react": "12.1.2",
        "@types/classnames": "2.3.1",
        "@types/history": "4.7.8",
        "@types/jest": "27.5.0",
        "@types/node": "10.17.60",
        "@types/react": "16.14.8",
        "@types/react-dom": "16.9.13",
        "@types/react-redux": "7.1.16",
        "@types/react-router": "4.4.5",
        "@types/react-router-dom": "4.3.5",
        "@types/react-test-renderer": "16.9.5",
        "@types/react-transition-group": "2.9.2",
        "@types/redux-actions": "2.3.1",
        "@types/webpack": "4.41.29",
        "@types/webpack-env": "1.13.6",
        "@typescript-eslint/eslint-plugin": "4.33.0",
        "@typescript-eslint/parser": "4.33.0",
        "babel-jest": "28.1.0",
        "body-parser": "1.19.0",
        "concurrently": "4.1.2",
        "eslint": "7.24.0",
        "eslint-config-prettier": "6.15.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-functional": "3.7.2",
        "eslint-plugin-import": "2.25.4",
        "eslint-plugin-react-hooks": "4.3.0",
        "express": "4.17.1",
        "file-loader": "3.0.1",
        "jest": "28.1.0",
        "jest-environment-jsdom": "28.1.0",
        "prettier": "1.19.1",
        "react-hot-loader": "4.13.0",
        "react-test-renderer": "16.14.0",
        "tsc-watch": "2.4.0",
        "url-loader": "1.1.2",
        "webpack-bundle-analyzer": "4.5.0",
        "webpack-cleanup-plugin": "0.5.1",
        "webpack-dev-server": "3.11.2",
        "webpack-merge": "5.8.0"
    },
    "dependencies": {
        "@babel/core": "7.14.3",
        "@types/lodash.clonedeep": "4.5.6",
        "@types/react": "16.14.8",
        "antd": "4.18.7",
        "babel-loader": "8.2.2",
        "canvas": "^2.9.1",
        "copyfiles": "2.4.1",
        "css-loader": "2.1.1",
        "gulp": "4.0.2",
        "highcharts": "9.3.3",
        "highcharts-react-official": "3.1.0",
        "i18next": "19.9.2",
        "konva": "8.3.2",
        "less": "2.7.2",
        "less-loader": "4.0.4",
        "react": "16.14.0",
        "react-dom": "16.14.0",
        "react-konva": "17.0.2-5",
        "react-router": "4.3.1",
        "react-router-dom": "5.2.0",
        "style-loader": "0.23.1",
        "ts-loader": "5.4.5",
        "typescript": "3.9.9",
        "use-image": "1.0.2",
        "webpack": "5.36.2",
        "webpack-cli": "4.7.0"
    },
    "jest": {
        "transformIgnorePatterns": [
            "node_modules/(?!(konva))"
        ],
        "testEnvironment": "jsdom"
    }
}
